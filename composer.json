{
    "name": "inventive/laravel-owner",
    "description": "Allow Eloquent models to own each other. Supports many to many relationships.",
    "type": "library",
    "authors": [
        {
            "name": "Stephen",
            "email": "stephenahiggins@icloud.com"
        }
    ],
    "require": {
        "inventive/laravel-owner": "dev-master"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Inventive\\LaravelOwner\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Inventive\\LaravelOwner\\OwnerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
